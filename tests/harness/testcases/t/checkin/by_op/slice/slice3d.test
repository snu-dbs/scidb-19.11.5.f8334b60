--setup
--start-query-logging
create array M10x10x10 <a:double> [x=1:10,2,0, y=1:10,2,0, z=1:10,2,0]
store(build(M10x10x10, (x*10+y)*10+z), M10x10x10)
scan(M10x10x10)  -- Inserted by scan_doctor

--test
slice(M10x10x10, y, 5)

slice(M10x10x10, x, 1)

slice(M10x10x10, y, 1)

join(slice(M10x10x10, x, 1), slice(M10x10x10, y, 1))

apply(join(slice(M10x10x10, x, 1) as M1, slice(M10x10x10, y, 1) as M2), delta, M2.a - M1.a)

--cleanup
remove (M10x10x10)

